# mount point		fstype		device				 	[flags] 
/misc				emmc		/dev/block/mmcblk0p20 	
/recovery			emmc		/dev/block/mmcblk0p19 	
/boot				emmc		/dev/block/mmcblk0p16 	 flags=display="Kernel";backup=1
/cache				ext4		/dev/block/mmcblk0p18 	
/data				ext4		/dev/block/mmcblk0p21	 length=-16384		
/system				ext4		/dev/block/mmcblk0p17 	
/external_sd    		vfat        	/dev/block/mmcblk1p1     /dev/block/mmcblk1    flags=display="Micro SD";storage;removable
